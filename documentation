document.querySelector("h1").style.color = "red"  ==  $("h1").css("color","red");

///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

$(document).ready(function(){
    $("h1").css("color","red");
})
VERIFICA SE A BIBLIOTECA ESTÁ PRONTA, ANTES DE ABRIR

///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

$("h1").addClass("big-tittle");
ADICIONA UMA CLASS A TAG HTML 

///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

$("h1").addClass("big-tittle person-margin")
ADICIONA DUAS CLASSES A TAG HTML

///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

$("h1").hasClass("person-margin")
VERIFICA SE O HTML POSSUI A CLASS "PERSON-MARGIN"

///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

$("h1").text("End");
ALTERA O TEXTO DE H1 PARA "END"

///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

$("button").html("<i>Next</i>");
ALTERA A TAG HTML, ACRESCENTANDO "<i>Next</i>" [INNERHTML]

///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

$("a").attr("href", "https://github.com/Johnaits/JQuery/new/main");
ADICIONA O "HREF" E O LINK NA TAG "A"

///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

$("h1").click(function()    {
    $("h1").text("hello!");
})
ADICIONA UM EVENTO OUVINTE A TAG H1

///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

$("input").keypress(function(event){
    console.log(event);
})
INTERPRETA A TECLA PRESSIONADA NO INPUT E MOSTRA NO CONSOLE

///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

$(document).keypress(function(event){
    console.log(event.key);
});
INTERPRETA A TECLA PRESSIONADA NO SITE E MOSTRA NO CONSOLE

///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

$(document).keypress(function(event){
    $("h1").text((event.key));
});
A CADA CLICK DO TECLADO, A TECLA É ESCRITA NO H1

///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

$(document).on("mouseover",function(){
    $("button").css("color","pink");
})
QUANDO O MOUSE SE MOVE, O TEXTO DO BUTTON FICA ROSA

///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

$("h1").on("click",function(){
    $("h1").before("<button>Good</button>");
})
AO CLICAR EM "H1", A FUNÇÃO É CHAMADA E ELA DIZ QUE DEVE CRIAR UM BUTTON ANTES DO H1

///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

$("h1").on("click",function(){
    $("h1").after("<button>Good</button>");
})
AO CLICAR EM "H1", A FUNÇÃO É CHAMADA E ELA DIZ QUE DEVE CRIAR UM BUTTON APÓS DO H1

